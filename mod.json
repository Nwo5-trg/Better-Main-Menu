	"developer": "Nwo5",
	"description": "Revamps the main menu",
	"dependencies": [
        {
            "id": "geode.node-ids",
			"version": ">=v1.19.0",
			"importance": "required"
        },
		{
            "id": "geode.texture-loader",
			"version": ">=v1.7.0",
			"importance": "recommended"
        }
    ],
	"incompatibilities": [
        {
            "id": "ninxout.redash",
			"version": ">=v1.2.0",
			"importance": "conflicting"
        }
    ],
	"tags": [
		"interface",
		"customization",
		"enhancement",
		"offline"
	],
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"links": {
		"source": "https://github.com/Nwo5-trg/Better-Main-Menu"
	},
